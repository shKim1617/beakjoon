   "source": [
    "n, k = map(int, input().split())\n",
    "count = 0\n",
    "flag = True\n",
    "\n",
    "for division_n in range(1, n+ 1):\n",
    "    if n % division_n == 0:\n",
    "        count += 1\n",
    "    if count == k:\n",
    "        print(division_n)\n",
    "        flag = False\n",
    "        break\n",
    "\n",
    "if flag:\n",
    "    print(0)"
   ]
  }
